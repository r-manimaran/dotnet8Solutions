{
  "Logging": {
    "LogLevel": {
      "Default": "Information",
      "Microsoft.AspNetCore": "Warning"
    }
  },
  "AllowedHosts": "*",
  "ReverseProxy":{
    "Routes":{
      "first-weather-routes": {
        "clusterId":"first-weather",
        "match": {
          "path":"{**catch-all}"
        }
      }
    },
    "Clusters":{
      "first-weather":{
        "destinations":{
          "destination1":{
            "address":"http://localhost:5144"
          }
        }
      }
    }
  }
}
